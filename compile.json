{
    "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
	"environment": {
		"qx.icontheme": "Tango",
		"excludeFromAPIViewer": [
			"qx.*",
			"qxl.*"
		]
	},
	"applications": [
		{
			"class": "com.zenesis.qx.upload.demo.Application",
			"name": "uploadmgr.demo",
			"title": "uploadmgr.demo",
			"theme": "com.zenesis.qx.upload.demo.theme.Theme",
			"default": true
		}
	],
	"targets": [
		{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en"
	],
	"eslintConfig": {
		"parserOptions": {
			"ecmaVersion": 2017
		},
		"rules": {}
	},
	"sass": {
	   "compiler": "legacy"
	}
}
